{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "mock-local-storage": "^1.0.5",
    "nock": "^9.0.28",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.16",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reset-css": "^2.2.1",
    "sinon": "^4.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
